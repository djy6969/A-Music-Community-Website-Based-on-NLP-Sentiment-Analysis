{
  "name": "vue-netease-muisc",
  "version": "3.4.2",
  "private": true,
  "scripts": {
    "dev": "npm run serve",
    "serve": "SET NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "build": "SET NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build",
    "publish": "npm run build && cd music && now",
    "lint": "vue-cli-service lint",
    "release-f": "standard-version -f",
    "release-major": "standard-version -r major",
    "release-minor": "standard-version -r minor",
    "release-patch": "standard-version -r patch",
    "release": "sh release.sh"
  },
  "dependencies": {
    "@better-scroll/core": "^2.0.0-alpha.20",
    "@better-scroll/mouse-wheel": "^2.0.0-alpha.20",
    "@better-scroll/scroll-bar": "^2.0.0-alpha.20",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^2.0.10",
    "@jiaminghi/data-view": "^2.10.0",
    "axios": "^0.21.1",
    "better-scroll": "^1.15.2",
    "bootstrap": "^5.2.3",
    "clipboard": "^2.0.4",
    "core-js": "^2.6.5",
    "echarts": "^5.4.2",
    "element-ui": "^2.15.13",
    "rtc-audio-mixer": "^0.1.1",
    "trtc-js-sdk": "latest",
    "good-storage": "^1.1.0",
    "lodash-es": "^4.17.15",
    "sass": "^1.56.0",
    "socket.io-client": "2.3.0",
    "vue": "^2.6.10",
    "vue-cookies": "^1.8.3",
    "vue-lazyload": "^1.3.3",
    "vue-meta": "^2.2.1",
    "vue-router": "^3.0.3",
    "vue-socket.io": "3.0.1",
    "vuex": "^3.0.1",
    "workbox-webpack-plugin": "^5.1.3",
    "xgplayer": "^2.1.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "commitizen": "^4.0.3",
    "concurrently": "^4.1.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "now": "^17.0.4",
    "postcss-pxtorem": "^4.0.1",
    "sass-loader": "^7.1.0",
    "standard-changelog": "^2.0.13",
    "standard-version": "^8.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": "off"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-pxtorem": {
        "rootValue": 14,
        "propList": [
          "*"
        ]
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
